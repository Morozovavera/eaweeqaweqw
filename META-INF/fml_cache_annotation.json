  "sekelsta/horse_colors/config/HorseConfig$Genetics": {
    "name": "sekelsta/horse_colors/config/HorseConfig$Genetics",
    "annotations": [
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "useGeneticStats",
        "value": {
          "values": [
            "If enabled, horses\u0027 speed, jump, and health will be determined",
            "through genetics instead of the default Minecraft way"
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$LangKey;",
        "target": "useGeneticStats",
        "value": {
          "value": "horse_colors.config.common.useGeneticStats"
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "enableHealthEffects",
        "value": {
          "values": [
            "If enabled, certain genes will have a small impact on health,",
            "as they do in real life. This does not prevent Overo Lethal",
            "White Syndrome."
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$LangKey;",
        "target": "enableHealthEffects",
        "value": {
          "value": "horse_colors.config.common.enableHealthEffects"
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "mutationChance",
        "value": {
          "values": [
            "The chance for each allele to mutate. The recommended range",
            "is between 0.0001 and 0.01.",
            "To disable mutations, set this value to 0."
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$RangeDouble;",
        "target": "mutationChance",
        "values": {
          "max": {
            "type": "DOUBLE",
            "value": "1.0"
          },
          "min": {
            "type": "DOUBLE",
            "value": "0.0"
          }
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "bookShowsGenes",
        "value": {
          "values": [
            "Enable or disable genetic testing."
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "bookShowsTraits",
        "value": {
          "values": [
            "Enable or disable physical inspection (rough information about health, ",
            "speed, and jump)."
          ]
        }
      }
    ]
  },
