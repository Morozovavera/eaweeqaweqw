  "sekelsta/horse_colors/config/HorseConfig$Breeding": {
    "name": "sekelsta/horse_colors/config/HorseConfig$Breeding",
    "annotations": [
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "enableGenders",
        "value": {
          "values": [
            "Enables or disables all features relating to gender."
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "genderlessBreedingCooldown",
        "value": {
          "values": [
            "The number of ticks until horses can breed again, when genders are disabled.",
            "The vanilla value is 6000 (or at 20 ticks per second, 5 minutes,",
            "or at 24000 ticks per minecraft day, 1/4 day)"
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$RangeInt;",
        "target": "genderlessBreedingCooldown",
        "values": {
          "min": {
            "type": "INT",
            "value": "0"
          }
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "maleBreedingCooldown",
        "value": {
          "values": [
            "The number of ticks until male horses can breed again.",
            "The default value is 240 ticks (12 seconds)."
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$RangeInt;",
        "target": "maleBreedingCooldown",
        "values": {
          "min": {
            "type": "INT",
            "value": "0"
          }
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "femaleBreedingCooldown",
        "value": {
          "values": [
            "The number of ticks until female horses can breed again.",
            "The default value is 24000 ticks (20 minutes, or 1 minecraft day).",
            "This must always be at least as long as pregnancyLength."
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$RangeInt;",
        "target": "femaleBreedingCooldown",
        "values": {
          "min": {
            "type": "INT",
            "value": "0"
          }
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "pregnancyLength",
        "value": {
          "values": [
            "If genders are enabled, females will be pregnant for this many ticks.",
            "The default value is 24000 ticks (20 minutes, or 1 minecraft day).",
            "To disable pregnancy altogether, set this number to 0.",
            "Lowering this will not let female horses breed again sooner unless you",
            "also lower femaleRebreedTicks"
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$RangeInt;",
        "target": "pregnancyLength",
        "values": {
          "min": {
            "type": "INT",
            "value": "0"
          }
        }
      }
    ]
  },
  "sekelsta/horse_colors/config/HorseConfig$Common": {
    "name": "sekelsta/horse_colors/config/HorseConfig$Common",
    "annotations": [
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "horseDebugInfo",
        "value": {
          "values": [
            "If enabled, debugging information will appear on the screen when the",
            "player is holding a stick in their left hand and looks at a horse.",
            "For most users, it is probably better to leave this as false."
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$LangKey;",
        "target": "horseDebugInfo",
        "value": {
          "value": "horse_colors.config.common.horseDebugInfo"
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "enableGroundTie",
        "value": {
          "values": [
            "If enabled, horses will not wander off if they are wearing a saddle."
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$LangKey;",
        "target": "enableGroundTie",
        "value": {
          "value": "horse_colors.config.common.enableGroundTie"
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "autoEquipSaddle",
        "value": {
          "values": [
            "If enabled, right clicking a horse while holding a saddle or horse armor",
            "will equip it (as long as the horse isn\u0027t already wearing something in that slot)",
            "instead of opening the inventory."
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$LangKey;",
        "target": "autoEquipSaddle",
        "value": {
          "value": "horse_colors.config.common.autoEquipSaddle"
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "blockVanillaHorseSpawns",
        "value": {
          "values": [
            "If set to true, only horses created by this mod will spawn.",
            "This mainly affects newly generated areas."
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "convertVanillaHorses",
        "value": {
          "values": [
            "If enabled, each vanilla horse will be replaced by a horse",
            "from this mod.",
            "This matters for worlds where vanilla horses have already spawned",
            "or will spawn."
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$Comment;",
        "target": "horseSpawnMultiplier",
        "value": {
          "values": [
            "Larger numbers make horses more common, smaller numbers make them less common.",
            "1.0 makes them as common as in vanilla."
          ]
        }
      },
      {
        "type": "FIELD",
        "name": "Lnet/minecraftforge/common/config/Config$RangeDouble;",
        "target": "horseSpawnMultiplier",
        "values": {
          "max": {
            "type": "DOUBLE",
            "value": "1000.0"
          },
          "min": {
            "type": "DOUBLE",
            "value": "0.0"
          }
        }
      }
    ]
  },
